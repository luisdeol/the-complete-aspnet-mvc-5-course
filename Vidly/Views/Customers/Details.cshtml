@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<strong>Membership Type: </strong>@Model.MembershipType.Name
<br />
@if (Model.BirthDate != null)
{
    <strong>Birth date: </strong>@Model.BirthDate.Value.ToString("d")
}

